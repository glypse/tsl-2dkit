{
	"name": "three-tsl-lab",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"description": "TSL-based library for 2D shaders",
	"files": [
		"dist"
	],
	"main": "./dist/three-tsl-lab.umd.cjs",
	"module": "./dist/three-tsl-lab.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/three-tsl-lab.js",
			"require": "./dist/three-tsl-lab.umd.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"keywords": [
		"three.js",
		"three",
		"canvas",
		"webgl",
		"webgl2",
		"webgpu",
		"tsl",
		"three.js shading language",
		"texture",
		"generative art",
		"shaders",
		"shader",
		"shader art"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc --project tsconfig-build.json && vite build",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@eslint/css": "^0.14.1",
		"@eslint/js": "^9.39.1",
		"@eslint/json": "^0.14.0",
		"@eslint/markdown": "^7.5.1",
		"@prettier/plugin-oxc": "^0.0.4",
		"@types/culori": "^4.0.1",
		"@types/node": "^24.10.0",
		"@types/three": "^0.181.0",
		"eslint": "^9.39.1",
		"globals": "^16.5.0",
		"jiti": "^2.6.1",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "npm:rolldown-vite@7.2.0"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.2.0"
		}
	},
	"dependencies": {
		"culori": "^4.0.2",
		"three": "^0.181.0"
	}
}
