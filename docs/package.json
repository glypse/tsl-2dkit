{
	"name": "docs",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "pnpm run generate-demos && vite dev",
		"build": "pnpm run generate-demos && vite build",
		"preview": "pnpm run generate-demos && vite preview",
		"prepare": "pnpm run generate-demos && svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate-demos": "node scripts/generate-demos-list.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@vitejs/plugin-basic-ssl": "^2.1.3",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"mdsvex": "^0.12.6",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"peerDependencies": {
		"@types/node": "*",
		"globals": "*",
		"jiti": "*",
		"three": "*",
		"typescript": "*"
	}
}
